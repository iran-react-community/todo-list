{
	"presets": [
		"react-app",
		[
			"@emotion/babel-preset-css-prop",
			{
				"autoLabel": true,
				"labelFormat": "[local]"
			}
		]
	],
	"plugins": [
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		],
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		],
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "es",
				"style": true
			}
		]
	]
}
